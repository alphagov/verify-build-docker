pipeline {
    agent any
    triggers {
        cron('H H * * 1-5')
     }
    stages {
        stage('Build and Push') {
            when {
              brnach 'master'
            }
            environment {
              DOCKER_HUB=credentials('dockerhub-govukverify')
            }
            steps {
              sh './build.sh'
            }
        }
    }
}
       
